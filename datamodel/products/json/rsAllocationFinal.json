      "ROBOSTRATEGY_DATA"
    ],
    "datatype": "FITS",
    "filesize": "1 GB",
    "releases": [
      {
        "name": "WORK",
        "description": "SDSS unreleased data.  Represents any work-in-progress data.",
        "public": false,
        "release_date": "unreleased"
      }
    ],
