  path_kwargs:
  - run2d
  access_string: spAll_epoch = $BOSS_SPECTRO_REDUX/{run2d}/epoch/spAll-{run2d}.fits.gz
IPL4:
  in_sdss_access: true
  path_name: spAll_epoch
