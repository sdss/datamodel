DR19:
  in_sdss_access: true
  path_name: spFrame
  path_template: $BOSS_SPECTRO_REDUX/{run2d}/@pad_fieldid|@isplate|/spFrame-{br}{id}-{frame:0>8}.fits.gz
  path_kwargs:
  - id
  - run2d
  - frame
  - br
  - fieldid
  access_string: spFrame = $BOSS_SPECTRO_REDUX/{run2d}/{field}/spFrame-{br}{id}-{frame}.fits.gz
