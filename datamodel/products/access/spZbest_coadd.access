IPL4:
  in_sdss_access: true
  path_name: spZbest_coadd
  path_template: $BOSS_SPECTRO_REDUX/{run2d}/@spcoaddfolder|/@spcoaddgrp|/{coadd}@spcoaddobs|/{run1d}/spZbest-{coadd}@spcoaddobs|-{mjd}.fits
