datamodel_generate -f SDSSV_DA_df -p MWM_WHITEDWARF/da_white_dwarf_properties/{vers}/SDSSV_DA_df.fits -k vers=1.0.2
datamodel_update -f SDSSV_DA_df -r DR19
datamodel_generate -f eSDSS_DA_df -p MWM_WHITEDWARF/da_white_dwarf_properties/{vers}/eSDSS_DA_df.fits -k vers=1.0.2
datamodel_update -f eSDSS_DA_df -r DR19
datamodel_generate -f mdwarf_abundances -p MWM_MDWARF/elemental_abundances/mdwarf_abundances-{vers}.fits -k vers=1.0.1
