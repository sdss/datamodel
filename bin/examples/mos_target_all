#!/usr/bin/env python3
from txt_to_datamodel import load_datamodel_summary, update_datamodel, save_datamodel_yaml
release = 'dr18'
product = 'mos_target'
tables = ["mos_target_allwise", "mos_target_carton_to_target", "mos_target_catalog", "mos_target_catalog_to_allwise", "mos_target_catalog_to_catwise2020", "mos_target_catalog_to_glimpse", "mos_target_catalog_to_guvcat", "mos_target_catalog_to_legacy_survey_dr8", "mos_target_catalog_to_panstarrs1", "mos_target_catalog_to_sdss_dr13_photoobj_primary", "mos_target_catalog_to_skies_v2", "mos_target_catalog_to_skymapper_dr2", "mos_target_catalog_to_supercosmos", "mos_target_catalog_to_tic_v8", "mos_target_catwise2020", "mos_target_gaia_dr2_ruwe", "mos_target_gaia_dr2_source", "mos_target_gaia_unwise_agn", "mos_target_gaiadr2_tmass_best_neighbour", "mos_target_geometric_distances_gaia_dr2", "mos_target_glimpse", "mos_target_guvcat", "mos_target_legacy_survey_dr8", "mos_target_magnitude", "mos_target_panstarrs1", "mos_target_sdss_dr13_photoobj_primary", "mos_target_skies_v2", "mos_target_skymapper_dr2", "mos_target_supercosmos", "mos_target_target", "mos_target_tic_v8", "mos_target_twomass_psc", "mos_target_best_brightest", "mos_target_bhm_csc_v2", "mos_target_bhm_efeds_veto", "mos_target_bhm_rm_v0_2", "mos_target_cadence", "mos_target_cadence_epoch", "mos_target_carton", "mos_target_cataclysmic_variables", "mos_target_catalog_to_bhm_efeds_veto", "mos_target_catalog_to_bhm_rm_v0_2", "mos_target_catalog_to_sdss_dr16_specobj", "mos_target_catalog_to_tycho2", "mos_target_catalog_to_uvotssc1", "mos_target_catalog_to_xmm_om_suss_4_1", "mos_target_catalogdb_version", "mos_target_category", "mos_target_ebosstarget_v5", "mos_target_gaia_dr2_wd", "mos_target_instrument", "mos_target_mapper", "mos_target_sagitta", "mos_target_sdss_apogeeallstarmerge_r13", "mos_target_sdss_dr16_qso", "mos_target_sdss_dr16_specobj", "mos_target_targetdb_version", "mos_target_targeting_generation", "mos_target_tycho2", "mos_target_uvotssc1", "mos_target_xmm_om_suss_4_1", "mos_target_yso_clustering", "mos_target_zari18pms"]
for table in tables:
    file = '%s_%s.txt' % (release, table)
    table = '%s_%s' % (product, table)
    datamodel, summary = load_datamodel_summary(release = release, file = file)
    datamodel = update_datamodel(table = table, datamodel = datamodel, summary = summary)
    save_datamodel_yaml(table = table, datamodel = datamodel)


