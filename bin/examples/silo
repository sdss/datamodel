datamodel generate -f silo_eboss_detections -p EBOSS_SPECTRO_LENSING/silo/{run2d}/{run1d}/{silover}/silo_eboss_detections-{silover}.fits -k run2d=v5_13_0 -k run1d=v5_13_0 -k silover=1.0.1 -r DR16

datamodel generate -f silo_manga_detections -p MANGA_SPECTRO_LENSING/silo/{drpver}/{silover}/silo_manga_detections-{silover}.fits -k drpver=v3_1_1 -k silover=1.0.4 -r DR17

datamodel generate -f silo_manga_stack_data -p MANGA_SPECTRO_LENSING/silo/{drpver}/{silover}/{plate}/manga_{plate}_{ifu}_stack_data.fits -k drpver=v3_1_1 -k silover=1.0.4 -k plate=8606 -k ifu=6102 -r DR17
